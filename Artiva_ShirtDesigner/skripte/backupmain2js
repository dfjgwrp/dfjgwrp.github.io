	for (let i = 0; i < objects.length; i++){
		let obj = objects[i];
		//obj.setTop(0);
		//obj.setCoords();
		//obj.scaleToHeight(pdf_canvas.height)
		MenONeckDesign0SvgGroup.addWithUpdate(obj)
	} 
});

